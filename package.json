{
  "name": "D2RM",
  "version": "1.0.0",
  "description": "Free music streaming player",
  "repository": {
    "type": "git",
    "url": "https://github.com/d2rm/d2rm.git"
  },
  "main": "index.html",
  "window": {
    "width": 1096,
    "height": 700,
    "min_width": 600,
    "min_height": 400,
    "frame": false,
    "toolbar": false,
    "transparent": true,
    "icon": "assets/imgs/ico.png"
  },
  "single-instance": true,
  "dependencies": {
    "angular": "1.3.8",
    "angular-animate": "1.3.8",
    "angular-loading-bar": "^0.7.1",
    "angular-resource": "1.3.8",
    "angular-route": "1.3.8",
    "moment": "2.8.4",
    "nedb": "^1.1.1",
    "node-webkit-builder": "^1.0.6",
    "node-webkit-updater": "0.2.3",
    "request": "2.51.0",
    "underscore": "1.7.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.3.15",
    "gulp": "3.8.11",
    "gulp-concat": "2.4.2",
    "gulp-ignore": "1.2.1",
    "gulp-minify-css": "0.3.11",
    "gulp-sass": "1.3.3",
    "gulp-shell": "0.2.11",
    "gulp-util": "3.0.1",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-nodewebkit-launcher": "0.0.11"
  }
}
